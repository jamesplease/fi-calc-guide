<header class="header">
  {% if home !== true %}
    <button class="header_open">
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 48 48" style="--_mt-iconSize:1.4rem; width: var(--_mt-iconSize, 1em); height: var(--_mt-iconSize, 1em);"> <path d="M6 36h36v-4H6v4zm0-10h36v-4H6v4zm0-14v4h36v-4H6z"></path></svg>
    </button>
  {% endif %}
  <div class="nav_logo">
    <a href="/" class="nav_logoLink">
      <img
        src="/logo-small.png"
        alt="FI Calc"
        class="nav_logoLinkImg"
      />
    </a>
  </div>
  <div class="header_right">
    {% if home !== true %}
      <div class="header_search">
        <form action="/search" method="get">
          <div class="header_searchInputContainer">
            <svg class="header_searchIcon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 48 48" style="--_mt-iconSize:1.125rem; width: var(--_mt-iconSize, 1em); height: var(--_mt-iconSize, 1em);"> <path d="M31 28h-1.59l-.55-.55C30.82 25.18 32 22.23 32 19c0-7.18-5.82-13-13-13S6 11.82 6 19s5.82 13 13 13c3.23 0 6.18-1.18 8.45-3.13l.55.55V31l10 9.98L40.98 38 31 28zm-12 0c-4.97 0-9-4.03-9-9s4.03-9 9-9 9 4.03 9 9-4.03 9-9 9z"></path></svg>
            <input
              type="text"
              name="q"
              class="header_searchInput"
              placeholder="Search"
              autocomplete="off"
              inputmode="search"/>
          </div>
        </form>
      </div>
    {% endif %}
    <div class="header_cta">
      <a class="button button-micro" href="https://calculator.ficalc.app">
        Launch Calculator
      </a>
    </div>
  </div>
</header>